{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotify-factory"
		},
		"Ds_SqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Ds_SqlDatabase'"
		},
		"Ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Ls_AzureDataLakeStorage'"
		},
		"Ds_SqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifysql.database.windows.net"
		},
		"Ds_SqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify"
		},
		"Ds_SqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sql"
		},
		"Ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stoargespotify.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotify-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ds_SqlDatabase_password": {
						"value": "[parameters('Ds_SqlDatabase_password')]"
					},
					"Ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('Ls_AzureDataLakeStorage_accountKey')]"
					},
					"Ds_SqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('Ds_SqlDatabase_properties_typeProperties_server')]"
					},
					"Ds_SqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('Ds_SqlDatabase_properties_typeProperties_database')]"
					},
					"Ds_SqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('Ds_SqlDatabase_properties_typeProperties_userName')]"
					},
					"Ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('Ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}